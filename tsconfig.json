{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017", "esnext.asynciterable"],
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
          "outDir": "./dist",
          "rootDir": "./src"
        },
        "include": ["src"],
        "exclude": ["node_modules", "dist"]
    }